<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:description="Fluid Template for bv page"
          jcr:primaryType="cq:Template"
          jcr:title="Fluid BV Template"
          allowedPaths="[/content/demo(/.*)?]"
          ranking="{Long}2">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="/apps/demo/components/structure/fluid-bv-page">
        <par-1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="white"
                    selectmargin="withoutmargin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </layout_container_com>
        </par-1>
        <par-2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="white"
                    selectmargin="margin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="demo/components/content/title"
                            title="Our Expertise"/>
                </parsys>
            </layout_container_com>
        </par-2>
        <par-3
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="grey"
                    selectmargin="margin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="demo/components/content/title"
                            title="Our Success Stories"/>
                </parsys>
            </layout_container_com>
        </par-3>
        <par-4
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="white"
                    selectmargin="margin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="demo/components/content/title"
                            title="What We Think"/>
                </parsys>
            </layout_container_com>
        </par-4>
        <par-5
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="grey"
                    selectmargin="margin"
                    selecttopmargin="topmargin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="demo/components/content/title"
                            title="What We Do"/>
                </parsys>
            </layout_container_com>
            <layout_container_com_1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="grey"
                    selectmargin="withoutmargin"
                    selecttopmargin="withouttopmargin"/>
        </par-5>
        <par-6
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <layout_container_com
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="white"
                    selectmargin="margin">
                <parsys
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="demo/components/content/title"
                            title="Segments"/>
                </parsys>
            </layout_container_com>
            <layout_container_com_1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/layout-container-component"
                    selectbackground="white"
                    selectmargin="withoutmargin"
                    selecttopmargin="withouttopmargin"/>
        </par-6>
    </jcr:content>
</jcr:root>
