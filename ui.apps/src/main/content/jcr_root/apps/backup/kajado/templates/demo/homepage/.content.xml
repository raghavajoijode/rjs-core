<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:description="Homepage template"
          jcr:primaryType="cq:Template"
          jcr:title="Home page"
          allowedPaths="[\0]"
          ranking="{Long}1">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="/apps/demo/components/structure/homepage">
        <case-study-title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/title"
                    title="Case Studies">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
            </title>
        </case-study-title>
        <latest-thinking-title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/title"
                    title="Latest thinking">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
            </title>
        </latest-thinking-title>
        <social-media
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/content/title"
                    title="Social media">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
            </title>
        </social-media>
        <hero-banner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
        <ipar-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
            <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/structure/header"/>
        </ipar-header>
        <ipar-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/structure/footer"/>
            <sitemap
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="demo/components/structure/sitemap"/>
        </ipar-footer>
    </jcr:content>
</jcr:root>
