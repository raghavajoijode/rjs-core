<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <editcondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <canmodify
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cq/gui/components/renderconditions/canmodify"
                    path="${param.item}"/>
            <functional-page
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="rjs/demos/renderconditions/functional-page"/>
            <!--group-suffix="some-suffix_"-->
        </editcondition>
    </jcr:content>
</jcr:root>
